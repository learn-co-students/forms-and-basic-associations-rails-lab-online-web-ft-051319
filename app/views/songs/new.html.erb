<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.label :song_notes_1 %><br>
  <%= text_field_tag "song[note_contents][]", nil, id: "song_notes_1" %><br>
  <%= f.label :song_notes_2 %><br>
  <%= text_field_tag "song[note_contents][]", nil, id: "song_notes_2" %><br>

  <%= f.submit %>
<% end %>


